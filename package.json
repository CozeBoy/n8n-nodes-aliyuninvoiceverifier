{
  "name": "n8n-nodes-aliyuninvoiceverifier",
  "version": "1.0.0",
  "description": "n8n node for verifying Chinese VAT invoices via Aliyun OCR API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "aliyun",
    "invoice",
    "verification",
    "VAT",
    "OCR"
  ],
  "license": "MIT",
  "homepage": "https://github.com/CozeBoy/n8n-nodes-aliyuninvoiceverifier#readme",
  "author": {
    "name": "jiaoyanggege",
    "email": "tonull@foxmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CozeBoy/n8n-nodes-aliyuninvoiceverifier.git"
  },
  "bugs": {
    "url": "https://github.com/CozeBoy/n8n-nodes-aliyuninvoiceverifier/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint nodes credentials --ext .ts",
    "lint:fix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run test"
  },
  "files": [
    "dist",
    "credentials",
    "nodes"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AliyunInvoiceVerifierApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AliyunInvoiceVerifier/AliyunInvoiceVerifier.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=0.100.0"
  },
  "dependencies": {
    "@alicloud/credentials": "^2.4.2",
    "@alicloud/ocr-api20210707": "^3.1.3",
    "@alicloud/openapi-client": "^0.4.15",
    "@alicloud/tea-typescript": "^1.7.1",
    "@alicloud/tea-util": "^1.4.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
